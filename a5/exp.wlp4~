//
// x^n
//


int wain(int x, int n){
   int y =1;
   if(n ==0){
   	x =1;
   }else{}
   while(n>1){
	if(n%2 ==0){
	       x=x*x;
	       n = n/2;
	}else{
	       y = x*y;
	       x= x*x;
	       n=(n-1)/2;
	}
   }
   return x*y;
}


int ifeven(int a){
    int x=0;
    if(a%2==0){
    x=1;
    }else{}
    return x;
}

int hui(int a){
    a=1;
    return 1;	
}