int wain(int a, int b) {
    int x = 0;
    while(x < b + a + 5) {
        if(x < b+a) {
            println(x+b);
            if(x > a) {
                println(x-a);
            } else {
                println(x-b);
            }
        } else {
            println(x*b);
            if(x > b+a) {
                println(x-a+5);
            } else {
                println(x-b+3);
            }
        }
        if(x == a) {
            println(a*b-x);
        } else {
            println(x);
        }
        x = x + 1;
    }
    return(x);
}