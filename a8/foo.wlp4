int printArray(int * arr, int len) {
  int i = 0;
  while (i < len) {
    println(*(arr+i));
    i = i + 1;
  }
  return 0;
}

int foo(int times) {
  int * arr = NULL;
  int i = 0;
  if (times > 0) {
    arr = new int[10];
    while (i < 10) {
      *(arr+i) = times * i;
      i = i + 1;
    }
    i = foo(times-1);
    i = printArray(arr, 10);
    delete[] arr;    
  } else {}
  return 0;
}

int wain(int a, int b) {
  return foo(10);
}
